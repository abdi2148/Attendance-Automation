<?xml version="1.0" encoding="UTF-8"?>

<?import com.jfoenix.controls.JFXButton?>
<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.layout.BorderPane?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.text.Font?>

<BorderPane fx:id="borderPane" prefHeight="600.0" prefWidth="800.0" styleClass="table-view" stylesheets="@../css/style.css" xmlns="http://javafx.com/javafx/8.0.171" xmlns:fx="http://javafx.com/fxml/1" fx:controller="attendanceautomation.gui.controller.TeacherSceneController">
   <top>
      <HBox fx:id="hBoxNavigationBar" alignment="CENTER" prefHeight="70.0" prefWidth="200.0" styleClass="lblFlow" BorderPane.alignment="CENTER">
         <children>
            <Label prefHeight="21.0" prefWidth="67.0" />
            <JFXButton fx:id="btn_logout" buttonType="RAISED" onAction="#handle_Logout" prefHeight="31.0" prefWidth="116.0" text="Log out" textFill="WHITE" />
            <Label prefHeight="21.0" prefWidth="242.0" />
            <JFXButton fx:id="btn_controlpanel" buttonType="RAISED" onAction="#handle_ControlPanel" prefHeight="39.0" prefWidth="154.0" text="Control Panel" textFill="WHITE">
               <HBox.margin>
                  <Insets left="10.0" right="5.0" />
               </HBox.margin>
               <font>
                  <Font size="18.0" />
               </font>
            </JFXButton>
            <Label prefHeight="21.0" prefWidth="106.0" />
            <Label fx:id="lblTeacherName" prefHeight="21.0" prefWidth="177.0" styleClass="btnBarNameLabel">
               <HBox.margin>
                  <Insets left="50.0" />
               </HBox.margin>
            </Label>
         </children>
      </HBox>
   </top>
</BorderPane>
